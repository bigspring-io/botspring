  "scripts": {
    "jwt": "ts-node scripts/getJwt",
    "check-env": "ts-node scripts/checkVariables",
    "prepare": "is-ci || husky install",
    "clean": "rimraf .webpack .serverless",
    "serve": "sls offline start --stage local",
    "start": "docker compose up",
    "stop": "docker compose down",
    "test": "jest",
    "test:watch": "jest --watch",
    "typecheck": "tsc --noEmit -p .",
    "lint": "eslint --cache --ext .js,.ts .",
    "lint:fix": "eslint --fix --cache --ext .js,.ts .",
    "format:check": "prettier --check .",
    "format": "prettier --write .",
    "serverless": "sls",
    "build": "sls webpack --stage ${NODE_ENV:=development}",
    "build:development": "export SLS_DEBUG=* && sls webpack --stage development",
    "build:production": "sls webpack --stage production",
    "deploy": "sls deploy --stage ${NODE_ENV:=development}",
    "deploy:development": "export SLS_DEBUG=* && sls deploy --stage development",
    "deploy:production": "sls deploy --stage production"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com",
    "tag": "latest"
